script/source = "extends KinematicBody2D

var movimento = Vector2.ZERO
var temporizador = 0
var deuTempo = false
var contador = 0
var comecou = false

func _ready():
	pass
	
func _physics_process(delta):	
	if Global.iniciou:
		comecou = true
		print(contador)
		if contador >= Global.lista.size():
			trocarcena()	
		else:
			if Global.lista[contador] == \"esquerda\":
				movimento.x = -200
			elif Global.lista[contador] == \"direita\":
				movimento.x = 200
			elif Global.lista[contador] == \"cima\":
				movimento.y = -200
			elif Global.lista[contador] == \"baixo\":
				movimento.y = 200
		Global.iniciou = false
		
	if comecou:
		temporizador += 1
		print(temporizador)
		
	if temporizador >= 240:
		comecou = false
		movimento.x = 0
		movimento.y = 0
		temporizador = 0
		contador += 1
		Global.iniciou = true
#
	move_and_slide(movimento)

func trocarcena():
	get_tree().change_scene(\"res://Telas/Control.tscn\")
"

